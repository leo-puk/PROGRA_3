<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="CarritoItemsPortBinding" />
                <binding name="AdministradoresPortBinding" />
                <binding name="ProductosPortBinding" />
                <binding name="CarritosPortBinding" />
                <binding name="ClientesPortBinding" />
                <binding name="MovimientosStockPortBinding" />
                <binding name="UsuariosPortBinding" />
                <binding name="EnviosPortBinding" />
                <binding name="LocalizacionesPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/techShopperWS/Administradores"
                binding="basicHttpBinding" bindingConfiguration="AdministradoresPortBinding"
                contract="AdministradoresWS.Administradores" name="AdministradoresPort" />
            <endpoint address="http://localhost:8080/techShopperWS/Productos"
                binding="basicHttpBinding" bindingConfiguration="ProductosPortBinding"
                contract="ProductosWS.Productos" name="ProductosPort" />
            <endpoint address="http://localhost:8080/techShopperWS/Carritos"
                binding="basicHttpBinding" bindingConfiguration="CarritosPortBinding"
                contract="CarritosWS.Carritos" name="CarritosPort" />
            <endpoint address="http://localhost:8080/techShopperWS/CarritoItems"
                binding="basicHttpBinding" bindingConfiguration="CarritoItemsPortBinding"
                contract="CarritoItemsWS.CarritoItems" name="CarritoItemsPort" />
            <endpoint address="http://localhost:8080/techShopperWS/Clientes"
                binding="basicHttpBinding" bindingConfiguration="ClientesPortBinding"
                contract="ClientesWS.Clientes" name="ClientesPort" />
            <endpoint address="http://localhost:8080/techShopperWS/MovimientosStock"
                binding="basicHttpBinding" bindingConfiguration="MovimientosStockPortBinding"
                contract="MovimientosStockWS.MovimientosStock" name="MovimientosStockPort" />
            <endpoint address="http://localhost:8080/techShopperWS/Usuarios"
                binding="basicHttpBinding" bindingConfiguration="UsuariosPortBinding"
                contract="UsuariosWS.Usuarios" name="UsuariosPort" />
            <endpoint address="http://localhost:8080/techShopperWS/Envios"
                binding="basicHttpBinding" bindingConfiguration="EnviosPortBinding"
                contract="EnviosWS.Envios" name="EnviosPort" />
            <endpoint address="http://localhost:8080/techShopperWS/Localizaciones"
                binding="basicHttpBinding" bindingConfiguration="LocalizacionesPortBinding"
                contract="LocalizacionesWS.Localizaciones" name="LocalizacionesPort" />
        </client>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/></startup></configuration>
